package AVLTree;

public class test {
	public static int fragment5(int n) {
		int sum = 0;
		for(int i = 0; i<n; i++) {
			for (int j=0; j<n*n; j++) {
				sum++;
			}
		}
		return sum;
	}
	public static int fragment6(int n) {
		int sum = 0;
		for(int i=0; i<n; i++) {
			for(int j = 0; j<i; j++) {
				sum++;
			}
		}
		return sum;
	}
	public static void main(String[] args) {
		System.out.println(fragment6(4));
	}
}
